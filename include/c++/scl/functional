#pragma once

//Comparison Functors
#include "streamline/functional/functor/comparison.hpp"
namespace std {


	using equal_to = sl::functor::equal;
	using less = sl::functor::less;
}

//Identity functor
#include "streamline/functional/functor/identity.hpp"
namespace std {
	using identity = sl::functor::identity;
}


//Function support
#include "streamline/functional/invoke.hpp"
namespace std {
	using sl::invoke;
	using sl::invoke_r;
}