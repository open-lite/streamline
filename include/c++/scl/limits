#pragma once


#include "streamline/numeric/numeric_traits.hpp"
namespace std {
	template<typename T>
	struct numeric_limits : sl::numeric_traits<T> {
		consteval static T max() noexcept { return sl::numeric_traits<T>::max; }
		consteval static T min() noexcept { return sl::numeric_traits<T>::min; }
	};
}